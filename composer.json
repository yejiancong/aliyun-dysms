{
  "name": "songshenzong/aliyun-dysms",
  "description": "a extension Aliyun SMS SDK",
  "keywords": [
    "aliyun",
    "SMS",
    "dysms",
    "sdk",
    "Laravel"
  ],
  "support": {
    "issues": "https://github.com/songshenzong/aliyun-dysms/issues",
    "source": "https://github.com/songshenzong/aliyun-dysms"
  },
  "type": "library",
  "license": "Apache-2.0",
  "homepage": "https://songshenzong.com",
  "authors": [
    {
      "name": "Aliyuncs",
      "homepage": "https://www.aliyun.com"
    },
    {
      "name": "Songshenzong",
      "email": "i@songshenzong.com"
    }
  ],
  "require": {
    "php": ">=5.6.32"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "laravel/framework": "5.6.*"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Songshenzong\\AliyunSms\\": "src/",
      "Songshenzong\\AliyunSms\\Test\\": "tests/",
      "Aliyun\\": "aliyun-dysms-php-sdk/api_sdk/lib"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Songshenzong\\AliyunSms\\ServiceProvider"
      ]
    }
  }
}
